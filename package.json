{
  "name": "garage-sale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "api": "nodemon server/index.js",
    "prod": "http-serve dist -p 5000 -g -c0 -S -C cert.pem -K key.pem",
    "proxy": "; node server/httpProxy.js"
  },
  "dependencies": {
    "apollo-boost": "^0.1.14",
    "apollo-server-express": "^2.0.3",
    "cors": "^2.8.4",
    "graphql": "^0.13.2",
    "mongoose": "^5.2.9",
    "nodemon": "^1.18.3",
    "register-service-worker": "^1.0.0",
    "serve": "^9.6.0",
    "vue": "^2.5.16",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.19",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "babel-plugin-transform-imports": "^1.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.1.6",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-unmodified-loop-condition": "off",
      "indent": [
        "error",
        4
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
